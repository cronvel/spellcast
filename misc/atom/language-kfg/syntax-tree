
Confirmed supported tree:

comment
	line
		double-slash
		double-dash
		number-sign
		percentage
		character
	block
		documentation

constant
	numeric
		decimal
	character
		escape
	language
		null
		true
		false
	other
		object

entity
	name
		function
		type
			class
			instance
		tag
			inline
			structure
			block
		section
	other
		inherited-class
		attribute-name

invalid
	illegal
	deprecated

keyword
	control
	operator
		assignment
		logical
		comparison
	other

markup
	underline
		link
	bold
	heading
	italic
	list
		numbered
		unnumbered
	quote
	raw
	other

meta

storage
	type
		var
		class
		function
	modifier
		static
		final
		abstract

string
	quoted
		single
		double
		triple
		other
	unquoted
	interpolated
	regexp
	other

support
	function
	class
	type
	constant
	variable
	other

variable
	parameter
	language
	other
		object
			property (property which is an object)
		property (property with random type)

punctuation
	definition
		comment
			begin
			end
		variable
		string
			begin
			end
		function
			body
				begin
				end
		arguments
			begin
			end
		tag
			begin
			end
		entry
		terminator
			statement
		separator
			delimiter
	
	???:
	terminator
		statement
	separator
		delimiter

