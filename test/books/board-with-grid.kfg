
[[doctype spellcast/book]]

[board-model grid-board]
	groups:
		main:
			type: grid
	create-places:
		type: square
		group: main
		range:
			x:
				min: -2
				max: 2
			y:
				min: -2
				max: 2

[chapter a-chapter]
	[starting-scene a-scene]
		[create-board $board] grid-board
		[to-unit-test board] $board

