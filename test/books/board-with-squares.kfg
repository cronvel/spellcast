
[[doctype spellcast/book]]

[board-model square-board]
	groups:
		main:
			type: grid
			logical-coord-names:
				- x
				- y
	logical-coord-names:
		- x
		- y
	create-places:
		type: square
		group: main
		range:
			x:
				min: 0
				max: 2
			y:
				min: 0
				max: 2

[chapter a-chapter]
	[starting-scene a-scene]
		[create-board $board] square-board
		[to-unit-test board] $board

