
[[doctype spellcaster]]

[formula]
	simpleArray:
		- 13
		- 15
		- 8
	
	array:
		-	a: 23
			b:
				c: 45
		-	a: 16
			b:
				c: 42
		-	a: 19
			b:
				c: 54

[spell sort]
	[sort $simpleArray]
	[message] $> Array: ${simpleArray}[enum]
	
	[sort $simpleArray]
		order: asc
	[message] $> Array: ${simpleArray}[enum]
	
	[sort $simpleArray]
		order: desc
	[message] $> Array: ${simpleArray}[enum]
	
	[sort $array]
		key: $a
	[message] $> Array: ${array[0].a} ${array[1].a} ${array[2].a}
	
	[sort $array]
		key: $a
		order: asc
	[message] $> Array: ${array[0].a} ${array[1].a} ${array[2].a}
	
	[sort $array]
		key: $a
		order: desc
	[message] $> Array: ${array[0].a} ${array[1].a} ${array[2].a}

	[sort $array]
		key: $b.c
		order: asc
	[message] $> Array: ${array[0].a} ${array[1].a} ${array[2].a}
