
[[doctype spellcaster]]

[formula]
	simpleArray:
		- 13
		- 15
		- 8
	
	array:
		-	a: 23
			b:
				c: 45
		-	a: 16
			b:
				c: 42
		-	a: 19
			b:
				c: 54

[spell sort]
	[sort $simpleArray => $r] $this
	[message] $> Original: ${simpleArray}[enum]
	[message] $> Result: ${r}[enum]
	
	[sort $simpleArray => $r] $= - $this
	[message] $> Original: ${simpleArray}[enum]
	[message] $> Result: ${r}[enum]
	
	[sort $simpleArray] $this
	[message] $> Original: ${simpleArray}[enum]
	
	[sort $array => $r] $this.a
	[message] $> Result: ${r[0].a} ${r[1].a} ${r[2].a}
	
	[sort $array => $r] $= - $this.a
	[message] $> Result: ${r[0].a} ${r[1].a} ${r[2].a}

	[sort $array => $r] $this.b.c
	[message] $> Result: ${r[0].a} ${r[1].a} ${r[2].a}

