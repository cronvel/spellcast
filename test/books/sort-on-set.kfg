
[[doctype spellcast/spellbook]]

[formula]
	list: <Set>
		-	name: orange
			value: 17
		-	name: apple
			value: 13
		-	name: cabbage
			value: 11
		-	name: ananas
			value: 18
	list_: <Set>
		- orange
		- apple
		- cabbage
		- ananas

[spell sort]
	[sort $list => $sorted] $= $this.left.value - $this.right.value
	[message] $> Sorted size: ${sorted.length}
	[foreach $sorted => $item]
		[message] $> Sorted item: ${item.name}
